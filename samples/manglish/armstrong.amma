
nischayikkuka Armstrong(n) {
     original = n
     x = 0
    ee  (n > 0) sathyamaavanavare{
         digit = n % 10
        x = (x + digit * digit * digit)
        n = n / 10
    }
    ithu  (x == original) sathyamo{
        parayuka("Armstrong Number")
    } allenkil {
        parayuka("Not Armstrong Number")
    }
}
sweekarikkuka x

Armstrong(x)
